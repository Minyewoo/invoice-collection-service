{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "settings": {
        "import/resolver": {
            "jsconfig": {
                "config": "jsconfig.json"
            }
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "prettier/prettier": "error",
        "react/prop-types": "off",
        "indent": "off",
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off"
    }
}